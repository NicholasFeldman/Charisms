{
  "rules": {
    ".read": false,
    ".write": false,
    "quotes": {
      ".read": true,
      "$qid": {
        ".write": "auth != null && (!data.exists() || data.child('submittedBy').val() == auth.uid)",
        "likes": {
          "$lid": {
            ".write": "auth != null && $lid == auth.uid"
          }
        }
      }
    }
  }
}
